{
  "plugins": ["jsdoc-http-plugin"],
  "sources": {
    "includes": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "./doc",
    "template": "node_modules/clean-jsdoc-theme",
    "tutorials": "tutorials/",
    "readme": "README.md"
  }
}
